{
  "version": 2,
  "public": false,
  "name": "robertoentringer.com",
  "alias": ["robertoentringer", "robertoentringer.com", "www.robertoentringer.com"],
  "regions": ["bru1"],
  "builds": [{ "src": "src/**", "use": "@now/static" }],
  "routes": [
    { "src": "/assets/(.*)", "dest": "src/assets/$1" },
    { "src": "/assets/video/(.*)", "headers": { "cache-control": "max-age=31536000" } },
    { "src": "/(.*)", "dest": "src/$1" }
  ]
}